{
  "dirs": {
    "build": "build/_client",
    "config": "_config",
    "dest": "public/_client",
    "src": "_source"
  },
  "tests": {
    "configFile": "karma.conf.js",
    "reporters": {
      "production": [
        "teamcity"
      ],
      "dev": [
        "mocha"
      ]
    }
  },
  "paths": {
    "src": {
      "scripts": "_source/scripts/",
      "sass": "_source/styles/"
    },
    "dest": {
      "scripts": "public/_client/scripts/",
      "sass": "public/_client/styles/"
    }
  }
}
